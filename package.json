{
  "name": "lumiere",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "build-dev": "npx drizzle-kit generate && npx expo prebuild -p android && npx expo run:android",
    "build-prod": "npx drizzle-kit generate &&  npx expo prebuild -p android && npx expo run:android --variant release",
    "build-dev-clean": "npx drizzle-kit generate && npx expo prebuild -p android --clean && npx expo run:android",
    "build-prod-clean": "npx drizzle-kit generate && npx expo prebuild -p android --clean && npx expo run:android --variant release",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web"
  },
  "dependencies": {
